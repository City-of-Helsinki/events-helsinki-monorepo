    "msw": "^1.2.2",
    "next": "13.4.7",
    "next-i18next": "14.0.0",
    "next-with-apollo": "^5.3.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-datepicker": "^4.14.1",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-helsinki-headless-cms": "1.0.0-alpha164",
    "react-i18next": "13.0.1",
