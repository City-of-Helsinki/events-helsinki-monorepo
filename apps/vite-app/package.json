{
  "name": "vite-app",
  "version": "0.6.5",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./node_modules/.vite",
    "serve": "vite preview",
    "test": "echo 'No test present yet'",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/vite-app.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@events-helsinki/common-i18n": "workspace:^",
    "@events-helsinki/core-lib": "workspace:^",
    "@events-helsinki/ui-lib": "workspace:^",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@emotion/babel-plugin": "11.10.2",
    "@events-helsinki/eslint-config-bases": "workspace:^",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-istanbul": "0.23.1",
    "@vitest/ui": "0.23.1",
    "eslint": "8.22.0",
    "happy-dom": "6.0.4",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.2",
    "vite": "3.1.0",
    "vite-plugin-svgr": "2.2.1",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.1"
  }
}
