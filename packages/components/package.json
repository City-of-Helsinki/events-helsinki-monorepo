    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-polyfill-corejs3": "0.5.3",
    "core-js": "3.25.0",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "happy-dom": "6.0.4",
    "identity-obj-proxy": "^3.0.0",
