{
  "name": "events-helsinki-graphql-proxy-server",
  "description": "Events Helsinki GraphQL proxy.",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/events-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "docker:prune-cache": "docker buildx prune",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker-compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty runner",
    "docker:serve": "docker-compose -f docker-compose.yml --env-file .env up runner"
  },
  "dependencies": {
    "@apollo/datasource-rest": "4.3.2",
    "@apollo/server": "4.3.1",
    "@apollo/utils.keyvaluecache": "2.1.0",
    "@sentry/integrations": "7.28.1",
    "@sentry/node": "7.28.1",
    "accepts": "1.3.8",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql-depth-limit": "1.1.0",
    "lodash": "4.17.21",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "peerDependencies": {
    "graphql": "16.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.20.7",
    "@events-helsinki/eslint-config-bases": "workspace:^",
    "@types/accepts": "1.3.5",
    "@types/body-parser": "^1",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "eslint": "8.30.0",
    "husky": "8.0.2",
    "jest": "28.1.3",
    "nodemon": "2.0.20",
    "path": "0.12.7",
    "prettier": "2.8.1",
    "ts-jest": "28.0.8",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0"
  }
}
