{
  "name": "@events-helsinki/graphql-proxy-server",
  "version": "2.0.0",
  "private": true,
  "description": "Events Helsinki GraphQL proxy.",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker compose -f docker-compose.yml build --progress=tty runner",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:prune-cache": "docker buildx prune",
    "docker:serve": "docker compose -f docker-compose.yml --env-file .env up runner",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/events-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "pre-commit": "lint-fix",
    "prepare": "husky",
    "test": "cross-env TZ=Europe/Helsinki vitest run",
    "test:changed": "yarn test --changed",
    "test:coverage": "yarn test --coverage",
    "test:debug": "yarn test --inspect-brk ----no-file-parallelism",
    "test:staged": "cross-env TZ=Europe/Helsinki vitest related --run",
    "test:watch": "cross-env TZ=Europe/Helsinki vitest watch",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.4.1",
    "@apollo/server": "^4.12.2",
    "@apollo/utils.keyvaluecache": "4.0.0",
    "@as-integrations/express5": "1.1.2",
    "@sentry/browser": "9.47.1",
    "@sentry/integrations": "7.120.4",
    "@sentry/node": "9.47.1",
    "accepts": "1.3.8",
    "body-parser": "2.2.1",
    "cors": "2.8.5",
    "express": "5.1.0",
    "graphql-depth-limit": "1.1.0",
    "lodash": "4.17.21",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@events-helsinki/eslint-config-bases": "workspace:^",
    "@types/accepts": "1.3.7",
    "@types/body-parser": "^1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/express-serve-static-core": "5.1.0",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/lodash": "^4.17.21",
    "@types/node": "22.19.1",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.5.0",
    "@vitest/ui": "3.2.4",
    "cross-env": "7.0.3",
    "dotenv": "^17.2.3",
    "dotenv-expand": "12.0.3",
    "eslint": "9.39.1",
    "husky": "^9.1.7",
    "nodemon": "3.1.11",
    "prettier": "^3.7.0",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vitest": "3.2.4",
    "vitest-sonar-reporter": "2.0.4",
    "webpack": "5.103.0",
    "winston": "3.18.3"
  },
  "peerDependencies": {
    "graphql": "^16.11.0"
  }
}
