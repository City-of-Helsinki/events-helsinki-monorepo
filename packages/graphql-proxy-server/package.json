{
  "name": "@events-helsinki/graphql-proxy-server",
  "version": "2.0.0",
  "private": true,
  "description": "Events Helsinki GraphQL proxy.",
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty runner",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker-compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:prune-cache": "docker buildx prune",
    "docker:serve": "docker-compose -f docker-compose.yml --env-file .env up runner",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/events-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "pre-commit": "lint-fix",
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.0.1",
    "@apollo/server": "4.7.5",
    "@apollo/utils.keyvaluecache": "3.0.0",
    "@events-helsinki/components": "workspace:^",
    "@sentry/integrations": "7.56.0",
    "@sentry/node": "7.56.0",
    "accepts": "1.3.8",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql-depth-limit": "1.1.0",
    "lodash": "4.17.21",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.7",
    "@babel/plugin-syntax-flow": "7.27.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@events-helsinki/eslint-config-bases": "workspace:^",
    "@types/accepts": "1.3.7",
    "@types/body-parser": "^1",
    "@types/cors": "2.8.19",
    "@types/express": "4.17.23",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.19",
    "@types/node": "20.19.1",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "7.0.3",
    "dotenv": "^16.6.0",
    "dotenv-expand": "10.0.0",
    "eslint": "8.57.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "nodemon": "2.0.22",
    "path": "0.12.7",
    "prettier": "^2.8.8",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "webpack": "5.99.9",
    "winston": "3.17.0"
  },
  "peerDependencies": {
    "graphql": "^16.11.0"
  }
}
