{
  "name": "events-graphql-proxy",
  "description": "Events Helsinki GraphQL proxy.",
  "version": "0.2.10",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "build": "NODE_ENV=production webpack --progress --mode production --config config/webpack.config.js",
    "dev": "nodemon",
    "start": "node build",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/events-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "check-dist": "es-check -v",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "generate:graphql": "graphql-codegen",
    "docker:prune-cache": "docker buildx prune",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker-compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty runner",
    "docker:serve": "docker-compose -f docker-compose.yml --env-file .env up runner"
  },
  "dependencies": {
    "@apollo/server": "4.3.0",
    "@apollo/subgraph": "2.2.2",
    "@sentry/integrations": "7.28.1",
    "@sentry/node": "7.28.1",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/node": "18.11.18",
    "@types/uuid": "9.0.0",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "2.12.6",
    "lodash": "4.17.21",
    "promise": "8.3.0",
    "typescript": "4.9.4",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-operations": "2.5.10",
    "@graphql-codegen/typescript-resolvers": "2.7.10",
    "@types/body-parser": "^1",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.191",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-eslint": "10.1.0",
    "dotenv-expand": "10.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "jest": "28.1.3",
    "nodemon": "2.0.20",
    "path": "0.12.7",
    "prettier": "2.8.1",
    "ts-jest": "28.0.8",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
