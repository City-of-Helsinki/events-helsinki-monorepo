{
  "name": "events-graphql-proxy",
  "description": "Events Helsinki GraphQL proxy.",
  "version": "0.2.10",
  "private": true,
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "build": "NODE_ENV=production webpack --progress --mode production --config config/webpack.config.js",
    "start": "nodemon",
    "start:prod": "node build",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/events-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "check-dist": "es-check -v",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "generate:graphql": "graphql-codegen",
    "docker:prune-cache": "docker buildx prune",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker-compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty runner",
    "docker:serve": "docker-compose -f docker-compose.yml --env-file .env up runner"
  },
  "dependencies": {
    "@apollo/subgraph": "^2.1.4",
    "@sentry/integrations": "^6.6.0",
    "@sentry/node": "^6.6.0",
    "@types/node": "^18.11.9",
    "apollo-datasource-rest": "^0.14.0",
    "apollo-server": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "promise": "^8.1.0",
    "typescript": "<4.9.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "apollo-server-testing": "^2.25.3",
    "babel-eslint": "^10.1.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  }
}
