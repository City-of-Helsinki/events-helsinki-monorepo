{
  "name": "venue-graphql-proxy",
  "version": "1.5.0",
  "private": true,
  "description": "Venue GraphQL proxy.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --mode production --config config/webpack.config.js",
    "check-dist": "",
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "dev": "nodemon --exec 'ts-node' src/index.ts",
    "docker:build": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty runner",
    "docker:develop": "cross-env DOCKER_BUILDKIT=1 docker-compose -f ./docker-compose.yml up",
    "docker:develop:reinstall": "cross-env DOCKER_BUILDKIT=1 docker-compose -f docker-compose.yml build --progress=tty --no-cache --force-rm develop",
    "docker:install": "docker buildx bake -f docker-compose.proxy.yml --progress=tty deps",
    "docker:prune-cache": "docker buildx prune",
    "docker:serve": "docker-compose -f docker-compose.yml --env-file .env up runner",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/venue-graphql-proxy.eslintcache",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "pre-commit": "lint-fix",
    "prepare": "husky install",
    "start": "node build -r dotenv-expand/config",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.0.1",
    "@apollo/subgraph": "2.4.8",
    "@events-helsinki/components": "workspace:^",
    "@events-helsinki/graphql-proxy-server": "workspace:^",
    "chalk": "4.1.2",
    "date-fns": "^2.30.0",
    "graphql": "^16.11.0",
    "graphql-tag": "2.12.6",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/core": "7.27.7",
    "@babel/plugin-syntax-flow": "7.27.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@events-helsinki/eslint-config-bases": "workspace:^",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.19",
    "@types/node": "20.19.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "7.0.3",
    "dotenv": "^16.6.0",
    "dotenv-expand": "10.0.0",
    "eslint": "8.57.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "nodemon": "2.0.22",
    "path": "0.12.7",
    "prettier": "^2.8.8",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
